<robot name="GEM">
  <link name="base_link" />
  <link name="velodyne_1" />
  <link name="velodyne_2" />
  <link name="concat_velodyne" />
  <link name="front_esr" />
  <link name="rear_esr" />
  <link name="front_right_srr" />
