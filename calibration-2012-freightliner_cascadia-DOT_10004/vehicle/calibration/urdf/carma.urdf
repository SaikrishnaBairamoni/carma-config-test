  <link name="novatel" /> 
  <link name="gps" />
  <link name="novatel_imu"/>
  <link name="ned_heading"/>
  <link name="vehicle_front" />

  <!-- Reference Frame Transforms -->

  <!-- base_link -> vehicle_front Transform vehicle_front is FLU-->
  <joint name="base_link_to_vehicle_front" type="fixed">
		<parent link="base_link" />
		<child link="vehicle_front" />
    <origin xyz="7.32 0 0.71" rpy="0 0 0" />
  </joint>

  <!-- Sensor Transforms -->

  <joint name="velodyne_1" type="fixed" >
    <origin xyz="5.224 1.065 3.03" rpy="0 0 0.0676" />
    <parent link="base_link" />
    <child link="velodyne_1" />
  </joint>

 <joint name="velodyne_2" type="fixed" >
    <origin xyz="-0.136174 -2.06041 0.0183966" rpy="0.0005524 0.0001121 -0.0557507" />
    <parent link="velodyne_1" />
    <child link="velodyne_2" />
  </joint>

