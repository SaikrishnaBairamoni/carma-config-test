    <parent link="base_link" />
    <child link="rear_esr" />
  </joint>

  <joint name="front_right_srr" type="fixed" >
    <origin xyz="6.975 -1.235 0.705" rpy="0 0 -1.047" />
    <parent link="base_link" />
    <child link="front_right_srr" />
  </joint>

  <joint name="front_left_srr" type="fixed" >
    <origin xyz="6.972 1.26 0.70" rpy="0 0 0.9" />
    <parent link="base_link" />
    <child link="front_left_srr" />
  </joint>

  <joint name="rear_right_srr" type="fixed" >
    <origin xyz="2.06 -1.260 0.56" rpy="0 0 -1.6" />
    <parent link="base_link" />
    <child link="rear_right_srr" />
  </joint>

  <joint name="rear_left_srr" type="fixed" >
